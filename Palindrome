#include<stdio.h>
int main()
{
    int n,rem,rev=0,temp=0;
    printf("Enter the number :");
    scanf("%d",&n);
    temp=n;
    while(n>0){
        rem=n%10;
        rev=rev*10+rem;
        n=n/10;
    }
    if(temp==rev){
        printf("Given number is Palindrome");
    }
    else{
        printf("Given number is not Palindrome");
    }
}
